####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: QuickUsbDiag.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile QuickUsbDiag.pro
qmake: 
	@$(QMAKE) -o Makefile QuickUsbDiag.pro

dist: 
	@mkdir -p .obj/QuickUsbDiag && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/QuickUsbDiag/ && $(COPY_FILE) --parents QuickUSB_Logo.jpeg .obj/QuickUsbDiag/ && $(COPY_FILE) --parents AboutForm.ui.h QuickUsbDiag.ui.h .obj/QuickUsbDiag/ && ( cd `dirname .obj/QuickUsbDiag` && $(TAR) QuickUsbDiag.tar QuickUsbDiag && $(GZIP) QuickUsbDiag.tar ) && $(MOVE) `dirname .obj/QuickUsbDiag`/QuickUsbDiag.tar.gz . && $(DEL_FILE) -r .obj/QuickUsbDiag

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
		-$(DEL_FILE) .ui/qmake_image_collection.cpp
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/QuickUsbDiag.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/CQuickUsb.o: CQuickUsb.cpp stdafx.h \
		CQuickUsb.h \
		QuickUsb.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/CQuickUsb.o CQuickUsb.cpp

.ui/AboutForm.h: AboutForm.ui 
	$(UIC) AboutForm.ui -o .ui/AboutForm.h

.ui/AboutForm.cpp: .ui/AboutForm.h AboutForm.ui AboutForm.ui.h 
	$(UIC) AboutForm.ui -i AboutForm.h -o .ui/AboutForm.cpp

.ui/QuickUsbDiag.h: QuickUsbDiag.ui 
	$(UIC) QuickUsbDiag.ui -o .ui/QuickUsbDiag.h

.ui/QuickUsbDiag.cpp: .ui/QuickUsbDiag.h QuickUsbDiag.ui QuickUsbDiag.ui.h 
	$(UIC) QuickUsbDiag.ui -i QuickUsbDiag.h -o .ui/QuickUsbDiag.cpp

.obj/AboutForm.o: .ui/AboutForm.cpp AboutForm.ui.h \
		.ui/AboutForm.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/AboutForm.o .ui/AboutForm.cpp

.obj/QuickUsbDiag.o: .ui/QuickUsbDiag.cpp QuickUsbDiag.ui.h \
		.ui/QuickUsbDiag.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/QuickUsbDiag.o .ui/QuickUsbDiag.cpp

.obj/moc_AboutForm.o: .moc/moc_AboutForm.cpp  .ui/AboutForm.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_AboutForm.o .moc/moc_AboutForm.cpp

.obj/moc_QuickUsbDiag.o: .moc/moc_QuickUsbDiag.cpp  .ui/QuickUsbDiag.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_QuickUsbDiag.o .moc/moc_QuickUsbDiag.cpp

.moc/moc_AboutForm.cpp: $(MOC) .ui/AboutForm.h
	$(MOC) .ui/AboutForm.h -o .moc/moc_AboutForm.cpp

.moc/moc_QuickUsbDiag.cpp: $(MOC) .ui/QuickUsbDiag.h
	$(MOC) .ui/QuickUsbDiag.h -o .moc/moc_QuickUsbDiag.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: QuickUSB_Logo.jpeg
	$(UIC)  -embed QuickUsbDiag QuickUSB_Logo.jpeg -o .ui/qmake_image_collection.cpp

