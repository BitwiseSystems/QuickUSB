####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -fPIC -Wall -W -O2  -DQT_NO_DEBUG
CXXFLAGS = -pipe -fPIC -Wall -W -O2  -DQT_NO_DEBUG
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/local/qt/mkspecs/linux-g++ -I. -I$(QTDIR)/include -I/usr/local/qt/include -I.ui/ -I. -I.moc/
LINK     = g++
LFLAGS   =  
LIBS     = $(SUBLIBS) -L/usr/X11R6/lib $(QTDIR)/lib/libqt.a -L/usr/X11R6/lib  -lc /usr/lib/libquickusb.a /usr/lib/libusb.a -lXp -lXt -lX11 -lXext -lXinerama -lXrender  -lm -lSM -lICE -ldl
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic -L /usr/local/qtplugins
QMAKE    = $(QTDIR)/bin/qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = 
SOURCES = main.cpp \
		CQuickUsb.cpp
OBJECTS = .obj/main.o \
		.obj/CQuickUsb.o \
		.obj/QuickUsbDiag.o \
		.obj/outputForm.o \
		.obj/AboutForm.o \
		.obj/qmake_image_collection.o
FORMS = QuickUsbDiag.ui \
		outputForm.ui \
		AboutForm.ui
UICDECLS = .ui/QuickUsbDiag.h \
		.ui/outputForm.h \
		.ui/AboutForm.h
UICIMPLS = .ui/QuickUsbDiag.cpp \
		.ui/outputForm.cpp \
		.ui/AboutForm.cpp
SRCMOC   = .moc/moc_QuickUsbDiag.cpp \
		.moc/moc_outputForm.cpp \
		.moc/moc_AboutForm.cpp
OBJMOC = .obj/moc_QuickUsbDiag.o \
		.obj/moc_outputForm.o \
		.obj/moc_AboutForm.o
DIST	   = QuickUsbDiagStatic.pro
QMAKE_TARGET = QuickUsbDiagStatic
DESTDIR  = 
TARGET   = QuickUsbDiag

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( $(MAKE) )

Makefile: QuickUsbDiagStatic.pro  /usr/local/qt/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile QuickUsbDiagStatic.pro
qmake: 
	@$(QMAKE) -o Makefile QuickUsbDiagStatic.pro

dist: 
	@mkdir -p .obj/QuickUsbDiag && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/QuickUsbDiag/ && $(COPY_FILE) --parents QuickUSB_Logo.jpeg .obj/QuickUsbDiag/ && $(COPY_FILE) --parents QuickUsbDiag.ui.h outputForm.ui.h AboutForm.ui.h .obj/QuickUsbDiag/ && ( cd `dirname .obj/QuickUsbDiag` && $(TAR) QuickUsbDiag.tar QuickUsbDiag && $(GZIP) QuickUsbDiag.tar ) && $(MOVE) `dirname .obj/QuickUsbDiag`/QuickUsbDiag.tar.gz . && $(DEL_FILE) -r .obj/QuickUsbDiag

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
		-$(DEL_FILE) .ui/qmake_image_collection.cpp
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/QuickUsbDiag.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/CQuickUsb.o: CQuickUsb.cpp CQuickUsb.h \
		QuickUsb.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/CQuickUsb.o CQuickUsb.cpp

.ui/QuickUsbDiag.h: QuickUsbDiag.ui 
	$(UIC) QuickUsbDiag.ui -o .ui/QuickUsbDiag.h

.ui/QuickUsbDiag.cpp: .ui/QuickUsbDiag.h QuickUsbDiag.ui 
	$(UIC) QuickUsbDiag.ui -i QuickUsbDiag.h -o .ui/QuickUsbDiag.cpp

.ui/outputForm.h: outputForm.ui 
	$(UIC) outputForm.ui -o .ui/outputForm.h

.ui/outputForm.cpp: .ui/outputForm.h outputForm.ui 
	$(UIC) outputForm.ui -i outputForm.h -o .ui/outputForm.cpp

.ui/AboutForm.h: AboutForm.ui 
	$(UIC) AboutForm.ui -o .ui/AboutForm.h

.ui/AboutForm.cpp: .ui/AboutForm.h AboutForm.ui 
	$(UIC) AboutForm.ui -i AboutForm.h -o .ui/AboutForm.cpp

.obj/QuickUsbDiag.o: .ui/QuickUsbDiag.cpp .ui/QuickUsbDiag.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/QuickUsbDiag.o .ui/QuickUsbDiag.cpp

.obj/outputForm.o: .ui/outputForm.cpp .ui/outputForm.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/outputForm.o .ui/outputForm.cpp

.obj/AboutForm.o: .ui/AboutForm.cpp .ui/AboutForm.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/AboutForm.o .ui/AboutForm.cpp

.obj/moc_QuickUsbDiag.o: .moc/moc_QuickUsbDiag.cpp .ui/QuickUsbDiag.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_QuickUsbDiag.o .moc/moc_QuickUsbDiag.cpp

.obj/moc_outputForm.o: .moc/moc_outputForm.cpp .ui/outputForm.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_outputForm.o .moc/moc_outputForm.cpp

.obj/moc_AboutForm.o: .moc/moc_AboutForm.cpp .ui/AboutForm.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_AboutForm.o .moc/moc_AboutForm.cpp

.moc/moc_QuickUsbDiag.cpp: $(MOC) .ui/QuickUsbDiag.h
	$(MOC) .ui/QuickUsbDiag.h -o .moc/moc_QuickUsbDiag.cpp

.moc/moc_outputForm.cpp: $(MOC) .ui/outputForm.h
	$(MOC) .ui/outputForm.h -o .moc/moc_outputForm.cpp

.moc/moc_AboutForm.cpp: $(MOC) .ui/AboutForm.h
	$(MOC) .ui/AboutForm.h -o .moc/moc_AboutForm.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: QuickUSB_Logo.jpeg
	$(UIC)  -embed QuickUsbDiagStatic QuickUSB_Logo.jpeg -o .ui/qmake_image_collection.cpp

